{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false,
        "babelOptions": {
          "plugins": [
            "@babel/plugin-syntax-import-assertions"
          ]
        }
      },
    "rules": {
        "import/extensions": 0,
        "linebreak-style": 0
    }
}
